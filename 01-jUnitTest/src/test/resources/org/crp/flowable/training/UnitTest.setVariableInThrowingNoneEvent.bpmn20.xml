<?xml version="1.0" encoding="UTF-8"?>

<definitions xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
             xmlns:flowable="http://flowable.org/bpmn"
             targetNamespace="http://www.flowable.org/test">

    <process id="noneThrowingEventProcess">

        <startEvent id="start"/>
        <sequenceFlow id="flow1" sourceRef="start" targetRef="noneEvent"/>

        <intermediateThrowEvent id="noneEvent">
            <extensionElements>
                <flowable:executionListener class="org.crp.flowable.training.AddVariableListener" event="start" />
            </extensionElements>
        </intermediateThrowEvent>
        <sequenceFlow id="flow2" sourceRef="noneEvent" targetRef="end"/>

        <endEvent id="end"/>

    </process>

</definitions>
